<resources>

	<style name="AppTheme" parent="@style/AppCompatDark">
		<item name="android:windowDrawsSystemBarBackgrounds">true</item>
		<item name="android:colorPrimary">@color/colorPrimary</item>
		<item name="android:colorPrimaryDark">@color/colorPrimaryDark</item>
		<item name="android:colorAccent">@color/colorAccent</item>
	</style>
	<style name="AppTheme.Light" parent="@style/AppCompatLight">
		<item name="android:windowDrawsSystemBarBackgrounds">true</item>
		<item name="android:colorPrimary">@color/colorPrimaryL</item>
		<item name="android:colorPrimaryDark">@color/colorPrimaryDarkL</item>
		<item name="android:colorAccent">@color/colorAccentL</item>
	</style>

</resources>
